{
	"name": "@progsoc/fuzzjudge-server",
	"version": "0.2.10",
	"module": "main.ts",
	"type": "module",
	"bin": "dist/index.js",
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@types/bun": "1.2.14",
		"drizzle-kit": "0.31.0",
		"typescript": "5.8.3"
	},
	"types": "./dist",
	"engines": {
		"bun": ">=1.2.0"
	},
	"files": ["dist", "migrations", "README.md", "LICENSE"],
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts"
		},
		"./*": {
			"types": "./dist/src/*.d.ts"
		}
	},
	"dependencies": {
		"@asteasolutions/zod-to-openapi": "7.3.0",
		"@hono/swagger-ui": "0.5.1",
		"@hono/zod-openapi": "0.19.6",
		"@scalar/hono-api-reference": "0.8.5",
		"drizzle-orm": "0.43.1",
		"gray-matter": "4.0.3",
		"hono": "4.7.10",
		"remark": "15.0.1",
		"toml": "3.0.0",
		"zod": "3.25.23"
	},
	"scripts": {
		"build:types": "bun tsc -b",
		"build": "bun run build.ts",
		"dev": "bun run dev.ts ../sample"
	}
}
