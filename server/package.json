{
	"name": "server",
	"module": "main.ts",
	"type": "module",
	"devDependencies": {
		"@types/bun": "1.2.10",
		"drizzle-kit": "0.31.0"
	},
	"types": "./dist",
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js"
		},
		"./*": {
			"types": "./dist/src/*.d.ts",
			"import": "./dist/src/*.js"
		}
	},
	"peerDependencies": {
		"typescript": "5.8.3"
	},
	"dependencies": {
		"@asteasolutions/zod-to-openapi": "7.3.0",
		"@bokuweb/zstd-wasm": "0.0.27",
		"@hono/swagger-ui": "0.5.1",
		"@hono/zod-openapi": "0.19.5",
		"@scalar/hono-api-reference": "0.8.5",
		"@std/toml": "npm:@jsr/std__toml",
		"@std/yaml": "npm:@jsr/std__yaml",
		"drizzle-orm": "0.43.1",
		"gray-matter": "4.0.3",
		"hono": "4.7.7",
		"remark": "15.0.1",
		"toml": "3.0.0",
		"zod": "3.24.3"
	},
	"scripts": {
		"build": "bun build ./src/index.ts ./src/client.ts ./src/v2/client.ts --outdir=./dist/src --minify --target bun --sourcemap",
		"dev": "bun run ./src/index.ts --watch"
	}
}
